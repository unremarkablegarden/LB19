{
  "name": "wordless",
  "version": "2.0.0",
  "description": "Wordless asset compiler",
  "main": "index.js",
  "repository": "git@github.com:welaika/wordless.git",
  "author": "'weLaika' <info@welaika.com>",
  "license": "MIT",
  "scripts": {
    "server": "nf start",
    "build:dev": "webpack --debug --env.WL_ENV=development",
    "build:prod": "webpack -p --bail --env.WL_ENV=production",
    "clean:js": "rimraf assets/javascripts/**.js assets/javascripts/**.map",
    "clean:css": "rimraf assets/stylesheets/**.css assets/stylesheets/**.map",
    "clean:images": "rimraf assets/images/**.{png,gif,jpg,svg}",
    "clean:dist": "yarn clean:js && yarn clean:css && yarn clean:images"
  },
  "engines": {
    "node": ">=10.6.0"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "autoprefixer": "^9.4.10",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.5",
    "barba.js": "^1.0.0",
    "buble": "^0.19.6",
    "buble-loader": "^0.5.1",
    "bulma": "^0.7.4",
    "clipboard-polyfill": "^2.7.0",
    "cssnano": "^4.1.10",
    "jquery": "^3.3.1",
    "macy": "^2.4.0",
    "postcss-loader": "^3.0.0",
    "simple-line-icons": "^2.4.1",
    "sweetalert2": "^8.3.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack-dev-server": "^3.2.1"
  },
  "devDependencies": {
    "browser-sync": "^2.24.5",
    "browser-sync-webpack-plugin": "^2.2.2",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.3.1",
    "imagemin-webpack-plugin": "^2.1.5",
    "node-sass": "^4.9.2",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "peerDependencies": {}
}
